{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "amd": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  // "extends": "eslint:recommended",
  "plugins": [
    "angular"
  ],
  "globals":{
    "angular":true
  },
  "rules":{
    "semi": [2, "always"],
    "strict": 2,
    "semi-spacing": [0, {"before": false, "after": true}],
    "indent": [2, 2]
  }
}